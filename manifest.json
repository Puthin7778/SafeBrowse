{
    "manifest_version": 3,
    "name": "It's Good",
    "description": "Helps you to avoid accessing malicious websites by analyzing the website.",
    "version": "1.1",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "32": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "32": "icon.png",
      "128": "icon.png"
    },
    "host_permissions": [
      "https://ajax.googleapis.com/",
      "<all_urls>"
    ],
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "popup.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "icon.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "style.css"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "popup.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }